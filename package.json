{
  "name": "rnfeaturecli",
  "description": "Project Initializer",
  "version": "0.0.8",
  "author": "Ashrith K S",
  "bin": {
    "rnfeaturecli": "./bin/run"
  },
  "dependencies": {
    "@oclif/command": "^1.5.12",
    "@oclif/config": "^1.12.11",
    "@oclif/plugin-help": "^2.1.6",
    "chalk": "^2.4.2",
    "ejs": "^2.6.1",
    "figlet": "^1.2.1",
    "inquirer": "^6.2.2"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.0",
    "@oclif/test": "^1.2.4",
    "chai": "^4.2.0",
    "eslint": "^5.13.0",
    "eslint-config-oclif": "^3.1.0",
    "globby": "^8.0.2",
    "mocha": "^5.2.0",
    "nyc": "^14.0.0"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/src"
  ],
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "src/index.js",
  "oclif": {
    "commands": "./src/commands",
    "bin": "rnfeaturecli",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ashrithks/featurecli.git"
  },
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint .",
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "test": "nyc mocha --forbid-only \"test/**/*.test.js\"",
    "version": "oclif-dev readme && git add README.md"
  }
}